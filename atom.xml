    node[shape=record]
    store1 [label=&amp;quot;&amp;lt;f0&amp;gt; left|&amp;lt;f1&amp;gt; Some data store&amp;quot;];
    proc1 [label=&amp;quot;{&amp;lt;f0&amp;gt; 1.0|&amp;lt;f1&amp;gt; Some process here\n\n\n}&amp;quot; shape=Mrecord];
    enti1 [label=&amp;quot;Customer&amp;quot; shape=box];
    store1:f1 -&amp;gt; proc1:f0;
    enti1-&amp;gt; proc1:f0;
}&lt;/span&gt;&lt;br&gt;&lt;/pre&gt;&lt;/td&gt;&lt;/tr&gt;&lt;/table&gt;&lt;/figure&gt;
