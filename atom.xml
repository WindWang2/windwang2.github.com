    store1 [label=&amp;quot;&amp;lt;f0&amp;gt; left|&amp;lt;f1&amp;gt; Some data store&amp;quot;];
    proc1 [label=&amp;quot;{&amp;lt;f0&amp;gt; 1.0|&amp;lt;f1&amp;gt; Some process here\n\n\n}&amp;quot; shape=Mrecord];
    enti1 [label=&amp;quot;Customer&amp;quot; shape=box];
    store1:f1 -&amp;gt; proc1:f0;
    enti1-&amp;gt; proc1:f0;
}&lt;/span&gt;&lt;br&gt;&lt;/pre&gt;&lt;/td&gt;&lt;/tr&gt;&lt;/table&gt;&lt;/figure&gt;
&lt;/div&gt;

&lt;div class=&quot;org-src-container&quot;&gt;

&lt;figure class=&quot;highlight&quot;&gt;&lt;table&gt;&lt;tr&gt;&lt;td class=&quot;code&quot;&gt;&lt;pre&gt;&lt;span class=&quot;line&quot;&gt;digraph dfd2{
    node[shape=record]
    subgraph level0{
    enti1 [label=&amp;quot;Customer&amp;quot; shape=box];
    enti2 [label=&amp;quot;Manager&amp;quot; shape=box];
    }
    subgraph cluster_level1{
		    label =&amp;quot;Level 1&amp;quot;;
		    proc1 [label=&amp;quot;{&amp;lt;f0&amp;gt; 1.0|&amp;lt;f1&amp;gt; One process here\n\n\n}&amp;quot; shape=Mrecord];
		    proc2 [label=&amp;quot;{&amp;lt;f0&amp;gt; 2.0|&amp;lt;f1&amp;gt; Other process here\n\n\n}&amp;quot; shape=Mrecord];
		    store1 [label=&amp;quot;&amp;lt;f0&amp;gt;    |&amp;lt;f1&amp;gt; Data store one&amp;quot;];
		    store2 [label=&amp;quot;&amp;lt;f0&amp;gt;   |&amp;lt;f1&amp;gt; Data store two&amp;quot;];
		    {rank=same; store1, store2}

    }
    enti1 -&amp;gt; proc1
    enti2 -&amp;gt; proc2
    store1 -&amp;gt; proc1
    store2 -&amp;gt; proc2
    proc1 -&amp;gt; store2
    store2 -&amp;gt; proc1 
}&lt;/span&gt;&lt;br&gt;&lt;/pre&gt;&lt;/td&gt;&lt;/tr&gt;&lt;/table&gt;&lt;/figure&gt;
